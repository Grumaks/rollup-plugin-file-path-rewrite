{
    "name": "rollup-plugin-path-rewrite",
    "version": "1.0.2",
    "description": "Rollup plugin for rewriting the output file/directory path",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Grumaks/rollup-plugin-path-rewrite.git"
    },
    "author": "Maksim Grushchenko",
    "homepage": "https://github.com/Grumaks/rollup-plugin-path-rewrite#readme",
    "bugs": {
        "url": "https://github.com/Grumaks/rollup-plugin-path-rewrite/issues"
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/es/index.js",
    "exports": {
        "types": "./types/index.d.ts",
        "import": "./dist/es/index.js",
        "default": "./dist/cjs/index.js"
    },
    "scripts": {
        "build": "rm -rf dist && rollup -c",
        "eslint": "eslint \"{**/*,*}.{js,mjs,ts,}\"",
        "prettier": "prettier --write .",
        "prettier:check": "prettier --check .",
        "test": "jest",
        "typings": "tsc --project tsconfig.typings.json"
    },
    "files": [
        "dist",
        "!dist/**/*.map",
        "types",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "peerDependencies": {
        "rollup": "^1.20.0||^2.0.0||^3.0.0||^4.0.0"
    },
    "peerDependenciesMeta": {
        "rollup": {
            "optional": true
        }
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.1.5",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/jest": "^29.5.11",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "conventional-changelog-conventionalcommits": "^7.0.2",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "rollup": "^4.9.1",
        "semantic-release": "^22.0.12",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.3"
    },
    "types": "./types/index.d.ts",
    "keywords": [
        "rollup-plugin",
        "rollup",
        "plugin",
        "path",
        "rewrite",
        "replace",
        "output",
        "directory",
        "file",
        "path rewrite"
    ]
}
